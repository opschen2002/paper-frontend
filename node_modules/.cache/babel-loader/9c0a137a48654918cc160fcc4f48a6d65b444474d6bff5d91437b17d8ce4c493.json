{"ast":null,"code":"export default {\n  name: 'ScoreCard',\n  props: {\n    title: {\n      type: String,\n      default: '体测成绩'\n    },\n    scoreData: {\n      type: Object,\n      required: true\n    }\n  }\n};","map":{"version":3,"names":["name","props","title","type","String","default","scoreData","Object","required"],"sources":["C:\\Users\\陈章宇\\Desktop\\test\\分步骤完成\\v7\\frontend\\paper-frontend\\src\\components\\common\\ScoreCard.vue"],"sourcesContent":["<template>\n  <el-card class=\"score-card\">\n    <template #header>\n      <div class=\"card-header\">\n        <span>{{ title }}</span>\n        <slot name=\"header-right\"></slot>\n      </div>\n    </template>\n    <el-descriptions :column=\"2\" border>\n      <el-descriptions-item label=\"身高\">\n        {{ scoreData.height }} cm\n      </el-descriptions-item>\n      <el-descriptions-item label=\"体重\">\n        {{ scoreData.weight }} kg\n      </el-descriptions-item>\n      <el-descriptions-item label=\"50米跑\">\n        {{ scoreData.run50m }} 秒\n      </el-descriptions-item>\n      <el-descriptions-item label=\"立定跳远\">\n        {{ scoreData.longJump }} cm\n      </el-descriptions-item>\n      <el-descriptions-item label=\"肺活量\">\n        {{ scoreData.vitalCapacity }} ml\n      </el-descriptions-item>\n      <el-descriptions-item label=\"体前屈\">\n        {{ scoreData.sitAndReach }} cm\n      </el-descriptions-item>\n      <template v-if=\"scoreData.gender === 'male'\">\n        <el-descriptions-item label=\"1000米跑\">\n          {{ scoreData.run1000m }} 秒\n        </el-descriptions-item>\n        <el-descriptions-item label=\"引体向上\">\n          {{ scoreData.pullUp }} 个\n        </el-descriptions-item>\n      </template>\n      <template v-else>\n        <el-descriptions-item label=\"800米跑\">\n          {{ scoreData.run800m }} 秒\n        </el-descriptions-item>\n        <el-descriptions-item label=\"仰卧起坐\">\n          {{ scoreData.sitUp }} 个\n        </el-descriptions-item>\n      </template>\n      <el-descriptions-item label=\"总分\">\n        {{ scoreData.totalScore }}\n      </el-descriptions-item>\n    </el-descriptions>\n  </el-card>\n</template>\n\n<script>\nexport default {\n  name: 'ScoreCard',\n  props: {\n    title: {\n      type: String,\n      default: '体测成绩'\n    },\n    scoreData: {\n      type: Object,\n      required: true\n    }\n  }\n}\n</script>\n\n<style scoped>\n.score-card {\n  margin-bottom: 20px;\n}\n\n.card-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n</style>"],"mappings":"AAmDA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDC,SAAS,EAAE;MACTH,IAAI,EAAEI,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}